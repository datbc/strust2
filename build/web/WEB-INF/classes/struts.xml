<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
"http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
    <!-- Configuration for the default package. -->
    <package name="default" extends="struts-default">
        <action name="AddContact" class="action.ActionAddContact">
            <result name="success">ListContact.jsp</result>
            <result name ="input">AddContact.jsp</result>
        </action>
        <action name="AddGroup" class="action.AddGroupAction">
            <result name="success">listGroup.jsp</result>
        </action>
        
        <action name="DeleteContact" class="action.DeleteContactAction">
            <result name="success">ListContact.jsp</result>
        </action>
        
        <action name="UpdateContactGet" class="action.EditContactAction">
            <result name="success">updateContact.jsp</result>
        </action>
        <action name="UpdateContact" class="action.UpdateAction">
            <result name="success">ListContact.jsp</result>
        </action>
        
        
    </package>
</struts>
